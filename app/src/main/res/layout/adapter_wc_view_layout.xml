<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="33.3"
        android:orientation="vertical">

        <TextView
            android:id="@+id/wctextView1"
            android:layout_width="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_height="30dp"
            android:gravity="center"
            android:text="TextView" />
        <TextView
            android:id="@+id/wctextView2"
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:gravity="center"
            android:text="TextView" />
    </LinearLayout>


    <TextView
        android:id="@+id/wctextView3"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginLeft="100dp"
        android:textAlignment="center"
        android:text="TextView" />

    <TextView
        android:id="@+id/wctextView4"
        android:layout_width="70dp"
        android:layout_height="60dp"
        android:layout_marginLeft="180dp"
        android:text="TextView" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="33.3"
        android:layout_marginLeft="250dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/wctextView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <TextView
            android:id="@+id/wctextView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <TextView
            android:id="@+id/wctextView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="33.3"
        android:layout_marginLeft="330dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/wctextView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <TextView
            android:id="@+id/wctextView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <TextView
            android:id="@+id/wctextView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView" />
    </LinearLayout>



</RelativeLayout>